    "@tailwindcss/typography": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "@vitejs/plugin-vue": "^2",
    "@vue/eslint-config-typescript": "^10",
    "autoprefixer": "^10",
    "cypress": "^9.5",
    "eslint": "^8.10",
    "eslint-config-prettier": "^8.4",
    "eslint-plugin-import": "^2",
    "eslint-plugin-node": "^11.1",
    "eslint-plugin-promise": "^6.0",
    "eslint-plugin-security": "<2",
    "eslint-plugin-vue": "^8",
    "postcss": "^8.4",
    "postcss-import": "^14.0",
    "postcss-nesting": "^10",
    "tailwindcss": "^3",
    "typescript": "^4.6",
    "vite": "^2.9",
    "vite-plugin-dts": "^1.0",
    "vue": "^3.2",
    "wait-on": "^6.0"
